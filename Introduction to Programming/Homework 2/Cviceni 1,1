using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Cvičení_1._1
{
    class Program
    {
        static void Main(string[] args)
        {
            // Specificky aritmeticky vyraz

            //Deklarace promenich s inicializaci
            int x = 10;
            int y = 20;
            int z = 10;
            //Deklarace socinu a konecneho vysledku
            int soucin;
            int vysledek;
            //vynasobeni promene "y" a promene "z" a zapsani do soucinu
            soucin = y * z;
            //odecteni soucinu a promene "x" a zapsani do vysledku
            vysledek = soucin - x;
            // zobrazeni vysledku aritmeticke operace
            Console.WriteLine("Vysledek aritmetickeho vyrazu {0} * {1} - {2} je {3}", z, y, x, vysledek);
            Console.ReadLine();







        }
    }
}
